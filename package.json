  "license": "MIT",
  "homepage": "https://github.com/pedropiloto/oidc-rp-adapted",
  "dependencies": {
    "@solid/jose": "0.1.8",
    "@trust/json-document": "^0.1.4",
    "@trust/webcrypto": "0.9.2",
    "base64url": "^3.0.0",
    "node-fetch": "^2.1.2",
    "standard-http-error": "^2.0.1",
    "text-encoding": "^0.6.4",
    "whatwg-url": "^6.4.1"
    },
    "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "mocha": "^5.2.0",
    "nock": "^9.3.0",
    "sinon": "^5.1.0",
    "sinon-chai": "^3.1.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2"
    }
}
